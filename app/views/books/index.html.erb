<h1>Books</h1>


 <% @books.each do |f| %>
　<p>Title: <%= f.title %></p>
  <p>Body: <%= f.body %></p>
  <%= link_to "Show", book_path(f.id) %>
  <%= link_to "Edit", edit_book_path(f) %>
 <% end %>

 <%= form_with model: @book do |f| %>
 
  <h4>Title</h4>
  <%= f.text_field :title %>
  
  <h4>Body</h4>
  <%= f.text_area :body %>
  
  <%= f.submit 'Create Book' %>
  
<% end %>
　